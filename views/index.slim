doctype 5
html
  head
    meta charset="UTF-8"
    title osusume bot
    link rel="stylesheet" href="stylesheet.css"
    style src="http://code.jquery.com/jquery-latest.js"
  body
    h1 おすすめbot
    #content
      table.osusumes
        thead
          tr
            th Name
            th Pattern
            th Content
        tbody
          - for x in @osusumes
            tr
              td.name = x[:name]
              td.regexp = x[:regexp]
              td.content = x[:content]
